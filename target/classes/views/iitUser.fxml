<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="iit.controllers.IitUserController"
            prefHeight="400.0" prefWidth="600.0" styleClass="box">
    <VBox AnchorPane.topAnchor="20" AnchorPane.leftAnchor="180" spacing="20">
        <TextField fx:id="adminName" editable="true" focusTraversable="false" promptText="User Name"/>
        <PasswordField fx:id="adminPassword" editable="true" focusTraversable="false" promptText="Password"/>
        <HBox spacing="30">
            <Button text="Sgin Up" styleClass="blue-theme" onAction="#save"/>
        </HBox>
    </VBox>
</AnchorPane>
